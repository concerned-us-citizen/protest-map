{
  "name": "event-map",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "scrape": "tsx scripts/scrapeAllTabs.js",
    "scrapecached": "tsx scripts/scrapeAllTabs.js --use-cache",
    "update-prebuilt-data": "tsx scripts/scrapeAllTabs.js --updatePrebuiltData",
    "setup": "node scripts/downloadPrecinctsSpatialIndex.js",
    "dev": "vite dev",
    "build": "npm run check && npm run lint:strict && vite build",
    "lint": "eslint . --ext .js,.ts,.svelte",
    "lint:strict": "eslint . --ext .js,.ts,.svelte --max-warnings=0",
    "lint-data": "node scripts/lintData.js cache/events_raw.json",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/leaflet": "^1.9.18",
    "@types/node": "^22.15.17",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.16",
    "eslint-plugin-svelte": "3.0.0-next.13",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-eslint-parser": "1.0.0-next.13",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.0.0",
    "vite": "^6.2.6"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.9",
    "@turf/turf": "^7.2.0",
    "cheerio": "^1.0.0",
    "geojson-rbush": "^3.2.0",
    "globals": "^16.1.0",
    "leaflet": "^1.9.4",
    "title-case": "^4.3.2",
    "tsx": "^4.19.4",
    "typescript-eslint": "^8.32.1"
  }
}
